{
  "name": "customer-server",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc --build",
    "start:prod": "node ./dist/index.js",
    "start:dev": "NODE_ENV=development nodemon ./src/index.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:seed:local": "NODE_ENV=development npm run db:seed",
    "db:push": "drizzle-kit push",
    "db:push:local": "NODE_ENV=development npm run db:push",
    "db:studio:local": "NODE_ENV=development drizzle-kit studio",
    "test": "jest --coverage",
    "lint": "eslint ."
  },
  "author": "Neil Gardose",
  "license": "ISC",
  "description": "A customer serving APIs using express and typescript",
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.1",
    "drizzle-zod": "^0.5.1",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@flydotio/dockerfile": "^0.5.9",
    "@jest/globals": "^29.7.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "drizzle-kit": "^0.28.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  }
